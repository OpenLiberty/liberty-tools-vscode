{
  "extends": "tslint:recommended",
  "rules": {
    "prefer-for-of": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "indent": false,
    "max-line-length": false,
    "max-classes-per-file": false,
    "curly": true,
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "double",
      "avoid-escape",
      "avoid-template"
    ]
  },
  "defaultSeverity": "warning"
}